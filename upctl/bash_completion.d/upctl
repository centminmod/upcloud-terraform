_upctl_database()
{
    last_command="upctl_database"

    command_aliases=()

    commands=()
    commands+=("connection")
    commands+=("delete")
    commands+=("list")
    commands+=("plans")
    commands+=("show")
    commands+=("start")
    commands+=("stop")
    if [[ -z "${BASH_VERSION:-}" || "${BASH_VERSINFO[0]:-}" -gt 3 ]]; then
        command_aliases+=("shutdown")
        aliashash["shutdown"]="stop"
    fi
    commands+=("types")

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--client-timeout=")
    two_word_flags+=("--client-timeout")
