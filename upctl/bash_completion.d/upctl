    local_nonpersistent_flags+=("--help")
    local_nonpersistent_flags+=("-h")
    flags+=("--client-timeout=")
    two_word_flags+=("--client-timeout")
    two_word_flags+=("-t")
    flags+=("--config=")
    two_word_flags+=("--config")
    flags+=("--debug")
    flags+=("--force-colours")
