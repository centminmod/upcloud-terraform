    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--client-timeout=")
    two_word_flags+=("--client-timeout")
    two_word_flags+=("-t")
    flags+=("--config=")
    two_word_flags+=("--config")
    flags+=("--debug")
    flags+=("--force-colours")
    flags+=("--no-colours")
    flags+=("--output=")
    two_word_flags+=("--output")
    two_word_flags+=("-o")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

_upctl_root_command()
{
    last_command="upctl"

    command_aliases=()

    commands=()
    commands+=("account")
    commands+=("completion")
    commands+=("database")
    if [[ -z "${BASH_VERSION:-}" || "${BASH_VERSINFO[0]:-}" -gt 3 ]]; then
        command_aliases+=("db")
        aliashash["db"]="database"
    fi
    commands+=("ip-address")
    commands+=("loadbalancer")
    if [[ -z "${BASH_VERSION:-}" || "${BASH_VERSINFO[0]:-}" -gt 3 ]]; then
        command_aliases+=("lb")
        aliashash["lb"]="loadbalancer"
    fi
    commands+=("network")
    commands+=("router")
    commands+=("server")
    commands+=("storage")
    commands+=("version")
    commands+=("zone")

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--client-timeout=")
    two_word_flags+=("--client-timeout")
    two_word_flags+=("-t")
    flags+=("--config=")
    two_word_flags+=("--config")
    flags+=("--debug")
    flags+=("--force-colours")
    flags+=("--no-colours")
    flags+=("--output=")
    two_word_flags+=("--output")
    two_word_flags+=("-o")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

__start_upctl()
{
    local cur prev words cword split
    declare -A flaghash 2>/dev/null || :
    declare -A aliashash 2>/dev/null || :
    if declare -F _init_completion >/dev/null 2>&1; then
        _init_completion -s || return
    else
        __upctl_init_completion -n "=" || return
    fi

    local c=0
    local flag_parsing_disabled=
    local flags=()
    local two_word_flags=()
    local local_nonpersistent_flags=()
    local flags_with_completion=()
    local flags_completion=()
    local commands=("upctl")
    local command_aliases=()
    local must_have_one_flag=()
    local must_have_one_noun=()
    local has_completion_function=""
    local last_command=""
    local nouns=()
    local noun_aliases=()

    __upctl_handle_word
}

if [[ $(type -t compopt) = "builtin" ]]; then
    complete -o default -F __start_upctl upctl
else
    complete -o default -o nospace -F __start_upctl upctl
fi

# ex: ts=4 sw=4 et filetype=sh

